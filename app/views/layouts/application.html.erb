<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title(yield(:title)) %></title>
    <%= render 'layouts/rails_default' %>
    <%= render 'layouts/shim' %>
  </head>
  <body>
    <div class="navbar-wrapper">
      <%= render 'layouts/header' %>
    </div>
    <div id="main-wrapper">
      <div class="container">
        <%= render 'shared/flash_message' %>
        <%= yield %>
        <%= debug(params) if Rails.env.development? %>
      </div>
    </div>
  </body>
</html>